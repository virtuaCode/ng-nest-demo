{
  "name": "ng-nest-demo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run build",
    "build": "ng build --prod && tsc --project server/tsconfig.json",
    "build-start": "npm run build && npm start",
    "start": "node -r ./bin/config bin/server",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "concurrently \"ng serve --progress false --proxy-config proxy.conf.json\" \"npm run dev-server\"",
    "dev-server": "cd server && nodemon --ext ts -r tsconfig-paths/register -r ./src/config.ts --ignore *.spec.ts --exec ts-node --project \"./tsconfig.json\" src/server.ts",
    "test": "concurrently \"ng test --progress false\" \"npm run test-server\"",
    "test-ng": "ng test",
    "test-server": "cd server && nodemon --ext ts --exec mocha **/*.spec.ts --require ts-node/register --reporter spec"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@nestjs/common": "^4.0.0",
    "@nestjs/core": "^4.0.0",
    "@nestjs/microservices": "^4.0.0",
    "@nestjs/testing": "^4.0.0",
    "@nestjs/websockets": "^4.0.0",
    "angular2-jwt": "^0.2.3",
    "bcrypt": "^1.0.3",
    "class-transformer": "^0.1.7",
    "class-validator": "^0.7.2",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.0.1",
    "mongoose": "^4.11.11",
    "mysql": "^2.14.1",
    "ng2-semantic-ui": "^0.9.6",
    "q": "^1.5.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "typeorm": "0.0.11",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.2",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/mocha": "^2.2.43",
    "@types/mongodb": "^2.2.11",
    "@types/mongoose": "^4.7.21",
    "@types/node": "^6.0.88",
    "@types/q": "^1.0.5",
    "@types/sinon": "^2.3.4",
    "chai": "^4.1.2",
    "codelyzer": "~3.1.1",
    "concurrently": "^3.5.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.0",
    "protractor": "~5.1.2",
    "sinon": "^3.2.1",
    "ts-node": "^3.2.2",
    "tsconfig-paths": "^2.3.0",
    "tslint": "~5.3.2",
    "typescript": "^2.3.4"
  }
}
